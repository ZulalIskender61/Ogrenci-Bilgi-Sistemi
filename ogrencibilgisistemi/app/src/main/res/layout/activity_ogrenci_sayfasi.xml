<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/OgrenciSayfasi"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OgrenciSayfasi">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="104dp"
        android:layout_height="97dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="66dp"
        app:layout_constraintBottom_toTopOf="@+id/ogrenciSayfasiDuyuruEkrani"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dairesel_yuvarlark_gri" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="92dp"
        android:layout_height="23dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="24dp"
        android:freezesText="false"
        android:text="DUYURULAR"
        android:textAlignment="center"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/ogrenciSayfasiDuyuruEkrani"
        android:layout_width="302dp"
        android:layout_height="0dp"
        android:layout_marginBottom="76dp"
        android:background="#A39C9C"
        android:ems="10"
        android:textSize="20sp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />
</androidx.constraintlayout.widget.ConstraintLayout>